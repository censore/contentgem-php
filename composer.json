{
    "name": "gemcontent/php-sdk",
    "description": "Official PHP SDK for GemContent API - AI-powered content generation platform",
    "type": "library",
    "license": "MIT",
    "version": "2.3.0",
    "authors": [
        {
            "name": "GemContent Team",
            "email": "support@gemcontent.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GemContent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GemContent\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs src/ tests/",
        "cs-fix": "phpcbf src/ tests/",
        "stan": "phpstan analyse src/",
        "fix": "php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "gemcontent",
        "api",
        "sdk",
        "php",
        "content-generation",
        "ai"
    ],
    "homepage": "https://docs.gemcontent.com/api",
    "support": {
        "issues": "https://github.com/gemcontent/gemcontent-php/issues",
        "source": "https://github.com/gemcontent/gemcontent-php"
    }
} 